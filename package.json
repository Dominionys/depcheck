{
  "name": "@depcheck-rs-node/core",
  "repository": "https://github.com/Dominionys/depcheck-rs.git",
  "version": "0.0.13",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "napi": {
    "name": "depcheck-rs-node",
    "triples": {
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-linux-android",
        "aarch64-unknown-linux-gnu",
        "aarch64-unknown-linux-musl",
        "aarch64-pc-windows-msvc",
        "armv7-unknown-linux-gnueabihf",
        "x86_64-unknown-linux-musl",
        "x86_64-unknown-freebsd",
        "i686-pc-windows-msvc",
        "armv7-linux-androideabi"
      ]
    }
  },
  "license": "MIT",
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "prepublishOnly": "yarn build:ts && yarn copy-files && napi prepublish -t npm",
    "version": "napi version",
    "build:napi": "napi build --platform --cargo-name depcheck_rs_node --js ./node/src/binding.js --dts ./node/src/binding.d.ts -p depcheck-rs-node --release",
    "build:napi:debug": "napi build --platform --cargo-name depcheck_rs_node --js ./node/src/binding.js --dts ./node/src/binding.d.ts -p depcheck-rs-node",
    "build:ts": "tsc --project tsconfig.build.json",
    "build:ts:debug": "tsc --noEmit",
    "build": "yarn clean && yarn build:napi && yarn build:ts && yarn copy-files",
    "build:debug": "yarn build:napi:debug && yarn build:ts:debug",
    "test": "jest",
    "clean": "rimraf node/dist",
    "copy-files": "node copy.js"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.5.0",
    "@types/jest": "^27.4.1",
    "copy-files": "^0.1.0",
    "jest": "^27.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3"
  }
}
