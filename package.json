{
  "name": "@depcheck-rs-node/core",
  "repository": "https://github.com/Dominionys/depcheck-rs.git",
  "version": "0.0.11",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "name": "depcheck-rs-node",
    "triples": {
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-linux-android",
        "aarch64-unknown-linux-gnu",
        "aarch64-unknown-linux-musl",
        "aarch64-pc-windows-msvc",
        "armv7-unknown-linux-gnueabihf",
        "x86_64-unknown-linux-musl",
        "x86_64-unknown-freebsd",
        "i686-pc-windows-msvc",
        "armv7-linux-androideabi"
      ]
    }
  },
  "license": "MIT",
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --cargo-name depcheck_rs_node -p depcheck-rs-node --release",
    "build:debug": "napi build --platform --cargo-name depcheck_rs_node -p depcheck-rs-node",
    "prepublishOnly": "napi prepublish -t npm",
    "version": "napi version",
    "test": "jest",
    "compile": "tsc --noEmit"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.5.0",
    "@types/jest": "^27.4.1",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3"
  }
}
